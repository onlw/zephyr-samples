&i2c0 {
	bmi270: bmi270@69 {
		compatible = "bosch,bmi270";
		reg = <0x69>;
	};

	bmm150: bmm150@10 {
		compatible = "bosch,bmm150";
		status = "okay";
		reg = <0x10>;
	};
};

